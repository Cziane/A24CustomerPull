{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "czianedf"
		},
		"AzureSqlDatabaseLS_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabaseLS'"
		},
		"StorageAccountLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StorageAccountLS'"
		},
		"AzureSqlDatabaseLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "cziansql.database.windows.net"
		},
		"AzureSqlDatabaseLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "czianeerp"
		},
		"AzureSqlDatabaseLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dsti_admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "czianedf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseLS_password": {
						"value": "[parameters('AzureSqlDatabaseLS_password')]"
					},
					"StorageAccountLS_connectionString": {
						"value": "[parameters('StorageAccountLS_connectionString')]"
					},
					"AzureSqlDatabaseLS_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabaseLS_properties_typeProperties_server')]"
					},
					"AzureSqlDatabaseLS_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabaseLS_properties_typeProperties_database')]"
					},
					"AzureSqlDatabaseLS_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabaseLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}